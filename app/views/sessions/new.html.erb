<% provide(:title, "Sign in") %>

<div class="jumbotron jumbotron-bookstack">
<h1> Sign in </h1>
<div class="row">
  <div class="center span6 offset3">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :Email%>
      <%= f.text_field :email%>

      <%= f.label :Number%>
      <%= f.password_field :card_number%>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary" %>
    <% end %>

    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
  </div>
</div>
</div>
